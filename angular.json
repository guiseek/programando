{
  "version": 1,
  "projects": {
    "portal-open": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/portal/open",
      "sourceRoot": "apps/portal/open/src",
      "prefix": "webapp",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/portal-open/browser",
            "index": "apps/portal/open/src/index.html",
            "main": "apps/portal/open/src/main.ts",
            "polyfills": "apps/portal/open/src/polyfills.ts",
            "tsConfig": "apps/portal/open/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/portal/open/src/favicon.ico",
              "apps/portal/open/src/assets",
              {
                "glob": "**/*",
                "input": "libs/common/ui/kit/src/lib/assets",
                "output": "./assets"
              }
            ],
            "styles": ["apps/portal/open/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/common/ui/kit/src/lib/scss"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "portal-open:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "portal-open:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portal-open:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/portal/open/tsconfig.app.json",
              "apps/portal/open/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/portal/open/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/portal/open/jest.config.js",
            "tsConfig": "apps/portal/open/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/portal/open/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/portal-open/server",
            "main": "apps/portal/open/server.ts",
            "tsConfig": "apps/portal/open/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "environments/environment.ts",
                  "with": "environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "portal-open:build",
            "serverTarget": "portal-open:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "portal-open:build:production",
              "serverTarget": "portal-open:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "portal-open:build:production",
            "serverTarget": "portal-open:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "only": "hosting:portal-open"
          }
        }
      }
    },
