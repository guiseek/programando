                "output": "./assets"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/assets/src/assets",
                "output": "./assets"
              },
              "apps/blog/src/sitemap.xml",
              "apps/blog/src/manifest.webmanifest"
            ],
            "styles": ["apps/blog/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/common/ui/kit/src/lib/scss"]
            },
            "scripts": []
