{
  "name": "webapp",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "dev": "ng serve -o",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "build:elements": "ng build --prod --project elements --output-hashing none && node tools/builders/build-elements.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "serve:videos": "http-server ~/guiseek/seek-tech/angular",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "scully": "scully",
    "scully:serve": "npx scully serve",
    "scully:watch": "npx scully watch",
    "build:blog": "ng build --project blog --prod",
    "blog": "ng build blog --buildOptimizer --extractCss --extractLicenses --no-namedChunks --ngswConfigPath=apps/blog/ngsw-config.json --optimization --outputHashing=all --serviceWorker --no-sourceMap --statsJson --no-vendorChunk",
    "ci:blog": "npm run build:blog && npm run scully",
    "deploy": "firebase deploy --only hosting:blog",
    "open": "http-server -c-1 dist/static/apps/blog/ -o",
    "elms": "http-server -c-1 dist/apps/elements/ -p 7070 -o",
    "renew": "npm run build && npm run scully && npm run open",
    "feat": "git flow feature start",
    "write": "git checkout -b",
    "post": "ng g @scullyio/init:post --target=libs/blog/posts --meta-data-file='libs/blog/feature-shell/blog-post-template.yaml' --name"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/elements": "^9.1.0",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.0.0",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/platform-server": "9.0.0",
    "@angular/router": "9.0.0",
    "@angular/service-worker": "9.0.0",
    "@angular/youtube-player": "^9.1.0",
    "@nguniversal/express-engine": "9.0.0",
    "@nrwl/angular": "9.0.3",
    "@scullyio/init": "^0.0.23",
    "@scullyio/ng-lib": "latest",
    "@scullyio/scully": "latest",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "firebase": ">= 5.5.7 <8",
    "prismjs": "^1.19.0",
    "rxjs": "~6.5.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@nguniversal/builders": "^9.0.0",
    "@nrwl/cypress": "9.0.3",
    "@nrwl/jest": "9.0.3",
    "@nrwl/workspace": "9.0.3",
    "@types/express": "^4.17.0",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "concat": "^1.0.3",
    "cypress": "^3.8.2",
    "dotenv": "6.2.0",
    "eslint": "6.1.0",
    "firebase-tools": "^7.12.0",
    "fs-extra": "^9.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "24.1.0",
    "jest-preset-angular": "8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.4"
  }
}
